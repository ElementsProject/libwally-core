{
  "name": "wallyjs",
  "version": "0.0.0",
  "description": "",
  "main": "wally.js",
  "scripts": {
    "cmake-all": "run-s cmake-configure cmake-build",
    "cmake-configure": "run-script-os",
    "cmake-configure:win32": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_ELEMENTS=on",
    "cmake-configure:linux": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_ELEMENTS=on -g",
    "cmake-configure:darwin": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_ELEMENTS=on -g",
    "cmake-build": "cmake-js build -O build",
    "cmake-clean": "cmake-js clean -O build",
    "cmake-all-package": "run-s cmake-all-configure cmake-build",
    "cmake-all-repackage": "run-s cmake-clean cmake-all-configure cmake-build",
    "cmake-all-configure": "run-script-os",
    "cmake-all-configure:win32": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_ELEMENTS=on --CDENABLE_SWIG_PYTHON=off --CDENABLE_SWIG_JAVA=on --CDGENERATE_WALLY=on --CDENABLE_JS_WRAPPER=on",
    "cmake-all-configure:linux": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_ELEMENTS=on --CDENABLE_SWIG_PYTHON=on --CDENABLE_SWIG_JAVA=on --CDGENERATE_WALLY=on --CDENABLE_JS_WRAPPER=on TARGET_RPATH=./wallycore -g",
    "cmake-all-configure:darwin": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_ELEMENTS=on --CDENABLE_SWIG_PYTHON=on --CDENABLE_SWIG_JAVA=on --CDGENERATE_WALLY=on --CDENABLE_JS_WRAPPER=on TARGET_RPATH=./wallycore -g",
    "cmake-debug-all": "run-s cmake-debug-configure cmake-debug-build",
    "cmake-debug-build": "cmake-js build -O build -D",
    "cmake-debug-configure": "run-script-os",
    "cmake-debug-configure:win32": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_ELEMENTS=on --CDGENERATE_WALLY=on --CDENABLE_JS_WRAPPER=on -C -D",
    "cmake-debug-configure:linux": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_ELEMENTS=on --CDGENERATE_WALLY=on --CDENABLE_JS_WRAPPER=on TARGET_RPATH=./wallycore -g -D",
    "cmake-debug-configure:darwin": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_ELEMENTS=on --CDGENERATE_WALLY=on --CDENABLE_JS_WRAPPER=on TARGET_RPATH=./wallycore -g -D",
    "ctest": "cd build && ctest -C Release",
    "ctest-debug": "cd build && ctest -C Debug",
    "cmake-ctest": "run-s cmake-clean cmake-all-configure cmake-build ctest"
  },
  "author": "",
  "license": "MIT",
  "gypfile": true,
  "dependencies": {
    "big-integer": "^1.6.26",
    "bindings": "*",
    "cmake-js": "^5.2.0",
    "nan": "^2.11.0",
    "node-gyp": "^3.8.0",
    "tape": "^4.8.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "run-script-os": "^1.0.5"
  }
}
